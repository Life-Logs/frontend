{
  "explorer.confirmDelete": false,
  // "editor.formatOnSave": true,
  // "[javascript]": {
  //   "editor.formatOnSave": true
  // "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  // },
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  // "editor.codeActionsOnSave": {
  //   "source.fixAll.eslint": true
  // },
  "editor.minimap.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "git.terminalAuthentication": false,
  "window.commandCenter": true,
  "files.eol": "\n",
  "debug.allowBreakpointsEverywhere": true,
  "editor.stickyScroll.enabled": true,
  "html.format.contentUnformatted": "",
  "php.validate.executablePath": "",
  "php.suggest.basic": false,
  "typescript.format.enable": false,
  "gitlens.defaultDateShortFormat": "",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.list.mouseWheelScrollSensitivity": 0.5,
  "terminal.integrated.mouseWheelScrollSensitivity": 0.3,
  "editor.mouseWheelScrollSensitivity": 0.5,
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#5A5A5A"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
"diffEditor.ignoreTrimWhitespace": false,
"emmet.optimizeStylesheetParsing": false,
"emmet.showAbbreviationSuggestions": false,
"emmet.showExpandedAbbreviation": "never",
"prettier.singleQuote": true,
"prettier.jsxSingleQuote": true
}